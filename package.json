{
  "name": "font-ascii",
  "version": "1.1.7",
  "description": "A ascii font in JS, with colors!",
  "main": "dist/index.js",
  "scripts": {
    "test": "$(npm bin)/ava",
    "test:watch": "npm run test -- --watch",
    "start": "node index.js",
    "build": "$(npm bin)/babel ./src -d ./dist",
    "clean": "rm -r ./dist",
    "postinstall": "postinstall-build dist"
  },
  "bin": {
    "font-ascii": "./bin/cli.js"
  },
  "dependencies": {
    "colors": "^1.1.2",
    "lodash": "^4.17.4",
    "postinstall-build": "^3.0.0"
  },
  "devDependencies": {
    "ava": "^0.19.1",
    "babel-cli": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.5.1"
  },
  "ava": {
    "require": "babel-register",
    "babel": {
      "babelrc": true
    }
  },
  "author": "",
  "license": "ISC"
}
