{
  "name": "font-ascii",
  "version": "1.1.16",
  "description": "A ascii font in JS, with colors!",
  "main": "dist/index.js",
  "repository": "sauloxd/font-ascii",
  "scripts": {
    "test": "$(npm bin)/ava",
    "test:watch": "npm run test -- --watch",
    "start": "NODE_ENV=development babel-node bin/formPhrase.js hello",
    "build": "$(npm bin)/babel ./src -d ./dist",
    "clean": "rm -r ./dist"
  },
  "bin": {
    "font-ascii": "./bin/formPhrase.js"
  },
  "dependencies": {
    "colors": "^1.4.0",
    "inquirer": "^7.0.0",
    "ramda": "^0.26.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/node": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "ava": "^2.4.0",
    "babel-preset-es2015-node4": "^2.1.1"
  },
  "ava": {
    "require": "babel-register",
    "babel": {
      "babelrc": true
    }
  },
  "author": {
    "name": "Saulo Furuta",
    "email": "saulotoshi@gmail.com",
    "url": "github.com/sauloxd"
  },
  "license": "MIT"
}
