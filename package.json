    "inquirer": "^7.0.0",
    "ramda": "^0.26.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/node": "^7.0.0",
    "@babel/preset-env": "^7.6.3",
    "@babel/register": "^7.6.2",
    "ava": "^2.4.0",
    "axios": "^0.19.0",
    "babel-preset-es2015-node4": "^2.1.1",
    "eslint": "^6.6.0",
    "prettier": "1.18.2"
