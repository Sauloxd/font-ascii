{
  "name": "font-ascii",
  "version": "1.2.1",
  "description": "A ascii font in JS, with colors!",
  "main": "dist/index.js",
  "repository": "sauloxd/font-ascii",
  "scripts": {
    "test": "npx ava",
    "test:watch": "npm run test -- --watch",
